{
  "compilerOptions": {
    "target": "es2016",         
    "moduleResolution": "node",                         
    "forceConsistentCasingInFileNames": true, 
    "jsx": "react-jsx",           
    "strict": true,                                      
    "skipLibCheck": true,
    "esModuleInterop": true                                 
  },
  "include": [
    "./src/**/*"
  ],
  "exclude": [
    "node_modules"
  ]
}
